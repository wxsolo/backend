body 
	background-color #f1f5f8
#main
	height 360px
	margin 0px
	text-align center
	background-color #000
	color #fff
	h1 
		font-size 70px
		margin-top 70px
	p
		font-size 25px
		margin-top 20px
	.loginby 
		margin-top 40px
		a 
			margin-left 10px

.active-men
	height 360px
	width 800px
	margin-top 30px
	background #eee
	margin 20px auto
	ul 
		width 225px
		height 360px
		padding-left 0px
		float left
		list-style none
		li 
			width 62px
			height 62px
			border-radius 31px
			margin-right 10px
			margin-bottom 10px
			float left
			background-color #fff
			a 
				width: 64px;
				height: 64px;
				display: block;
				position: relative;
				img 
					width 50px
					height 50px
					margin 6px 6px
					border-radius 25px
					transition .05s all ease-in-out
		li:hover a img  
			width 62px
			height 62px
			margin 0 0
			border-radius 31px
	.active-content
		width 500px
		height 350px
		float left
		margin 0px 25px
		border-radius 5px
		background-color #fff

